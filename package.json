    "@mui/lab": "^7.0.0-beta.13",
    "@mui/material": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.2",
    "chart.js": "^4.4.9",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "mongoose": "^8.15.1",
    "react-chartjs-2": "^5.3.0",
    "recharts": "^2.15.3"
  }
}